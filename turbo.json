{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "prebuild",
        "^build"
      ],
      "outputs": [
        "output-file.txt",
        "dist/**"
      ]
    },
    "@repo/python-lib#build": {
      "dependsOn": ["^build"],
      "outputs": ["../../dist/**", "uv.lock", "!__pycache__"]
    },
    "python-app#build": {
      "dependsOn": ["^build"],
      "outputs": ["../../dist/**", "uv.lock", "!__pycache__"]
    },
    "python-app2#build": {
      "dependsOn": ["^build", "@repo/python-lib#build"],
      "outputs": ["../../dist/**", "uv.lock", "!__pycache__"]
    },
    "prebuild": {},
    "lint": {},
    "fmt": {},
    "check-types": {}
  }
}
